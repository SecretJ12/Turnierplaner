{
	"name": "turnierplaner",
	"version": "0.0.0",
	"scripts": {
		"build": "vue-tsc --noEmit && vite build",
		"dev": "vite",
		"preview": "vite preview --port 5050",
		"lint:validate": "ESLINT_USE_FLAT_CONFIG=false eslint . --ext .vue,.ts,.tsx,.js,.jsx",
		"lint:fix": "ESLINT_USE_FLAT_CONFIG=false eslint . --ext .vue,.ts,.tsx,.js,.jsx --fix",
		"prettier:validate": "prettier . --check",
		"prettier:fix": "prettier . --write",
		"format:validate": "ESLINT_USE_FLAT_CONFIG=false eslint . --ext .vue,.ts,.tsx,.js,.jsx && prettier . --check",
		"format:fix": "ESLINT_USE_FLAT_CONFIG=false eslint . --ext .vue,.ts,.tsx,.js,.jsx --fix && prettier . --write"
	},
	"type": "module",
	"dependencies": {
		"@tanstack/vue-query": "^5.52.0",
		"@tanstack/vue-query-devtools": "^5.52.0",
		"@vee-validate/yup": "^4.13.2",
		"@vue/cli": "^5.0.8",
		"axios": "^1.6.2",
		"keycloak-js": "^23.0.3",
		"material-symbols": "^0.23.0",
		"oidc-client-ts": "^2.4.0",
		"primeflex": "^3.3.1",
		"primeicons": "^6.0.1",
		"primevue": "^3.44.0",
		"sass": "^1.69.5",
		"sortablejs": "^1.15.1",
		"uuid": "^9.0.1",
		"vee-validate": "^4.12.3",
		"vue": "^3.5.8",
		"vue-axios": "^3.5.2",
		"vue-cal": "^4.9.0",
		"vue-i18n": "^9.8.0",
		"vue-password": "^3.1.0",
		"vue-router": "^4.2.5"
	},
	"devDependencies": {
		"@types/sortablejs": "^1.15.7",
		"@types/uuid": "^9.0.7",
		"@typescript-eslint/eslint-plugin": "^6.14.0",
		"@typescript-eslint/parser": "^6.14.0",
		"@vitejs/plugin-vue": "^4.5.2",
		"@vue/cli-plugin-typescript": "~5.0.8",
		"@vue/cli-service": "^5.0.8",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-vue": "^9.19.2",
		"prettier": "^3.1.1",
		"typescript": "~5.6.2",
		"unplugin-auto-import": "^0.17.2",
		"unplugin-vue-components": "^0.26.0",
		"vite": "^5.0.10",
		"vue-component-type-helpers": "^2.1.6",
